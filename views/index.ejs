<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="css/index.css">
    <script src="js/JQuery.js"></script>
    <title>ChatApp</title>
</head>
<body>
    
    <!-- Log In -  -->
    <section class="mx-auto mt-5" style="width: 380px;">
        <div class="card text-center">
            <div class="card-header">
              <h2>Chat App</h2>
            </div>
            <div class="card-body">
                <form action="/login" method="post">
                    <div class="mb-3">
                      <label for="Name" class="form-label"></label>
                      <input type="text" class="form-control" name="Name" id="Name" placeholder="Username">
                      <!-- Remember to give Name attribute -->
                    </div>

                    <div class="mb-3">
                        <select name="Room" id="Room" class="form-select">
                            <option selected>Select Chat Room</option>
                            <option value="Alpha">Room Alpha</option>
                            <option value="Beta">Room Beta</option>
                            <option value="Gamma">Room Gamma</option>
                        </select>
                    </div>
                    
                    <div class="mb-3">
                        <button type="submit" id="login" class="btn btn-primary">Log In</button>
                    </div>
                </form>
            </div>
        </div>  
    </section>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
</body>
</html>